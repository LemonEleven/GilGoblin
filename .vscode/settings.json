{
  "githubPullRequests.ignoredPullRequestBranches": ["master"],
  "dotnet.defaultSolution": "GilGoblin.sln",
  "dotnet-test-explorer.autoExpandTree": true,
  "dotnet-test-explorer.testProjectPath": "**/tests/*Tests.csproj",
  "dotnet-test-explorer.treeMode": "merged",
  "dotnet-test-explorer.autoWatch": false,
  "omnisharp.enableRoslynAnalyzers": true,
  "omnisharp.enableEditorConfigSupport": true,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#47037a",
    "activityBar.background": "#47037a",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#9c5b04",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#47037a",
    "statusBar.background": "#2a0248",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#47037a",
    "statusBarItem.remoteBackground": "#2a0248",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#2a0248",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#2a024899",
    "titleBar.inactiveForeground": "#e7e7e799"
  }
